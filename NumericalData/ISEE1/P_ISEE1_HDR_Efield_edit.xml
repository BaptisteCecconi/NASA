<Spase xmlns="http://www.spase-group.org/data/schema">
    <Version>2.2.0</Version>
    <NumericalData>
        <ResourceID>spase://VSPO/NumericalData/SPMS-00238</ResourceID>
        <ResourceHeader>
            <ResourceName>ISEE 1 2-D, 3-sec Electric Field Data</ResourceName>
            <ReleaseDate>2008-02-20T00:00:00</ReleaseDate>
            <Description>of GSE X and Y components of the electric field; spacecraft potential, wave power in three frequency bands (3-14 Hz, 14-80 Hz, 120-600 Hz), spacecraft position in GSM, and 3-sec magnetic field data from the UCLA magnetometer.  Uncertainty in Ex is 1-2 mV/meter, and less in Ey.  Data values are binary integers.</Description>
            <Acknowledgement>Please acknowledge the PI, Dr. Forrest Mozer of UCB, and NSSDC</Acknowledgement>
            <Contact>
                <PersonID>spase://SMWG/Person/Cynthia.A.Cattell</PersonID>
                <Role>GeneralContact</Role>
            </Contact>
            <Contact>
                <PersonID>spase://SMWG/Person/Forrest.S.Mozer</PersonID>
                <Role>PrincipalInvestigator</Role>
            </Contact>
            <InformationURL>
                <Name>NSSDC Master Catalog</Name>
                <URL>http://nssdc.gsfc.nasa.gov/database/MasterCatalog?ds=SPMS-00238</URL>
            </InformationURL>
        </ResourceHeader>
        <AccessInformation>         <!-- &amp;amp;lt;RepositoryID&amp;amp;gt;vspo://spase/repository/0 &amp;amp;lt;/RepositoryID&amp;amp;gt; -->
            <RepositoryID>spase://SMWG/Repository/NASA/NSSDC</RepositoryID>
            <Availability>Offline</Availability>
            <AccessRights>Open</AccessRights>
            <AccessURL>
                <Name>NSSDC</Name>
                <URL>http://nssdc.gsfc.nasa.gov/nssdc/obtaining_data.html</URL>
            </AccessURL>
            <Format>Binary</Format>
            <DataExtent>
                <Quantity>2</Quantity>
                <Units>GB</Units>
            </DataExtent>
        </AccessInformation>
        <ProcessingLevel>Calibrated</ProcessingLevel>
        <InstrumentID>spase://SMWG/Instrument/ISEE1/LFEF</InstrumentID>
        <MeasurementType>ElectricField</MeasurementType>
        <TemporalDescription>
            <TimeSpan>
                <StartDate>1977-11-01T00:00:00</StartDate>
                <StopDate>1984-05-02T00:00:00</StopDate>
                <Note>Major data gaps: 1978-11-16 - 1980-02-12, 1982-05-06 - 1983-11-08</Note>
            </TimeSpan>
            <Cadence>PT3S</Cadence>
        </TemporalDescription>
        <ObservedRegion>Earth.Magnetosphere</ObservedRegion>
        <ObservedRegion>Earth.Magnetosheath</ObservedRegion>
        <ObservedRegion>Heliosphere.NearEarth</ObservedRegion>
        <InputResourceID/>
        <Parameter>
            <Name>Electric field X(GSE) component</Name>
            <Description>Electric field X(GSE) component</Description>
            <Cadence>PT3S</Cadence>
            <Units>mV/m</Units>
            <CoordinateSystem>
                <CoordinateRepresentation>Cartesian</CoordinateRepresentation>
                <CoordinateSystemName>GSE</CoordinateSystemName>
            </CoordinateSystem>
            <Field>
                <Qualifier>Component.I</Qualifier>
                <FieldQuantity>Electric</FieldQuantity>
            </Field>
        </Parameter>
        <Parameter>
            <Name>Electric field Y(GSE) component</Name>
            <Description>Electric field Y(GSE) component</Description>
            <Cadence>PT3S</Cadence>
            <Units>mV/m</Units>
            <CoordinateSystem>
                <CoordinateRepresentation>Cartesian</CoordinateRepresentation>
                <CoordinateSystemName>GSE</CoordinateSystemName>
            </CoordinateSystem>
            <Field>
                <Qualifier>Component.J</Qualifier>
                <FieldQuantity>Electric</FieldQuantity>
            </Field>
        </Parameter>
        <Parameter>
            <Name>Magnetic field vector</Name>
            <ParameterKey/>
            <Description>Cartesian components in GSE of magnetic field vector measured on UCLA magnetometer</Description>
            <Cadence>PT3S</Cadence>
            <Units>nT</Units>
            <CoordinateSystem>
                <CoordinateRepresentation>Cartesian</CoordinateRepresentation>
                <CoordinateSystemName>GSE</CoordinateSystemName>
            </CoordinateSystem>
            <Structure>
                <Size>3</Size>
                <Description>Magnetic field vector</Description>
            </Structure>
            <Field>
                <Qualifier>Vector</Qualifier>
                <FieldQuantity>Magnetic</FieldQuantity>
            </Field>
        </Parameter>
        <Parameter>
            <Name>Electric wave amplitude in 3 frequency bands</Name>
            <Description>Electric wave amplitude in the frequency bins 3-14 Hz, 14-80 Hz, 120-600 Hz</Description>
            <Cadence>PT3S</Cadence>
            <Units>microvolt/m-Hz**0.5</Units>
            <Structure>
                <Size>3</Size>
                <Description>Three E-field wave amplitudes</Description>
            </Structure>
            <Field>
                <Qualifier>Variance</Qualifier>
                <FieldQuantity>Electric</FieldQuantity>         <!--   &amp;amp;lt;FrequencyRange&amp;amp;gt;               	&amp;amp;lt;Low&amp;amp;gt;3&amp;amp;lt;/Low&amp;amp;gt;               	&amp;amp;lt;High&amp;amp;gt;600&amp;amp;lt;/High&amp;amp;gt;               	&amp;amp;lt;Units&amp;amp;gt;Hz&amp;amp;lt;/Units&amp;amp;gt;            &amp;amp;lt;/FrequencyRange&amp;amp;gt; -->
            </Field>
        </Parameter>
        <Parameter>
            <Name>Spacecraft potential</Name>
            <Description>Spacecraft potential</Description>
            <Cadence>PT3S</Cadence>
            <Units>Volts</Units>
            <Field>
                <FieldQuantity>Potential</FieldQuantity>
            </Field>
        </Parameter>
        <Parameter>
            <Name>Spacecraft location</Name>
            <Description>GSM Cartesian components of spacecraft position vector</Description>
            <Cadence>PT3S</Cadence>
            <Units>Re</Units>
            <CoordinateSystem>
                <CoordinateRepresentation>Cartesian</CoordinateRepresentation>
                <CoordinateSystemName>GSM</CoordinateSystemName>
            </CoordinateSystem>
            <Structure>
                <Size>3</Size>
                <Description>spacecraft position vector, X,Y,Z</Description>
            </Structure>
            <Support>
                <SupportQuantity>Positional</SupportQuantity>
            </Support>
        </Parameter>
    </NumericalData>
</Spase>
