<Spase xmlns="http://www.spase-group.org/data/schema">
    <Version>2.2.0</Version>
    <NumericalData>
        <ResourceID>spase://VSPO/NumericalData/P_MFI_FULL_1MIN_NSSDCFTP</ResourceID>
        <ResourceHeader>
            <ResourceName>Wind MFI 1-min magnetic field data</ResourceName>
            <ReleaseDate>2007-06-01T00:00:00</ReleaseDate>
            <Description>Magnetic field magnitude and GSE and GSM cartesian components at 1-min resolution, plus standard deviations in averages and Wind position data, in ASCII</Description>
            <Contact>
                <PersonID>spase://SMWG/Person/Ronald.P.Lepping</PersonID>
                <Role>PrincipalInvestigator</Role>
            </Contact>
            <Contact>
                <PersonID>spase://SMWG/Person/Adam.Szabo</PersonID>
                <Role>DataProducer</Role>
            </Contact>
            <InformationURL>
                <Name>Wind MFI web site</Name>
                <URL>http://wind.nasa.gov/mfi/</URL>
            </InformationURL>
        </ResourceHeader>
        <AccessInformation>
            <RepositoryID>spase://SMWG/Repository/NASA/NSSDC</RepositoryID>
            <Availability>Online</Availability>
            <AccessRights>Open</AccessRights>
            <AccessURL>
                <URL>ftp://nssdcftp.gsfc.nasa.gov/spacecraft_data/wind/mag/1min_ascii</URL>
            </AccessURL>
            <Format>Text</Format>
        </AccessInformation>
        <ProcessingLevel>Calibrated</ProcessingLevel>
        <InstrumentID>spase://SMWG/Instrument/WIND/MFI</InstrumentID>
        <MeasurementType>MagneticField</MeasurementType>
        <TemporalDescription>
            <TimeSpan>
                <StartDate>1994-11-16T00:00:00</StartDate>
                <RelativeStopDate>-P1Y</RelativeStopDate>
            </TimeSpan>
            <Cadence>PT1M</Cadence>
        </TemporalDescription>      <!--InstrumentRegion=Heliosphere.NearEarth-->
        <ObservedRegion>Heliosphere.NearEarth</ObservedRegion>
        <Parameter>
            <Name>GSE Cartesian components</Name>
            <ParameterKey>GSE Cartesian components</ParameterKey>
            <Description>Magnetic field cartesian components in GSE</Description>
            <Cadence>PT1M</Cadence>
            <Units>nT</Units>
            <CoordinateSystem>
                <CoordinateRepresentation>Cartesian</CoordinateRepresentation>
                <CoordinateSystemName>GSE</CoordinateSystemName>
            </CoordinateSystem>
            <Measured>
                <Field>
                    <FieldQualifier>Average</FieldQualifier>
                    <FieldQualifier>Component</FieldQualifier>
                    <FieldQualifier>Vector</FieldQualifier>
                    <FieldQuantity>Magnetic</FieldQuantity>
                </Field>
            </Measured>
        </Parameter>
        <Parameter>
            <Name>GSM Cartesian components</Name>
            <ParameterKey>GSM Cartesian components</ParameterKey>
            <Description>Y,Z components of magnetic field vector.  Note that Bx(GSM) = Bx(GSE)</Description>
            <Cadence>PT1M</Cadence>
            <Units>nT</Units>
            <CoordinateSystem>
                <CoordinateRepresentation>Cartesian</CoordinateRepresentation>
                <CoordinateSystemName>CGM</CoordinateSystemName>
            </CoordinateSystem>
            <Measured>
                <Field>
                    <FieldQualifier>Average</FieldQualifier>
                    <FieldQualifier>Component</FieldQualifier>
                    <FieldQualifier>Vector</FieldQualifier>
                    <FieldQuantity>Magnetic</FieldQuantity>
                </Field>
            </Measured>
        </Parameter>
        <Parameter>
            <Name>Field magnitude</Name>
            <ParameterKey>Field magnitude</ParameterKey>
            <Description>1-min average of field magnitudes</Description>
            <Cadence>PT1M</Cadence>
            <Units>nT</Units>
            <Measured>
                <Field>
                    <FieldQualifier>Magnitude</FieldQualifier>
                    <FieldQualifier>Average</FieldQualifier>
                    <FieldQuantity>Magnetic</FieldQuantity>
                </Field>
            </Measured>
        </Parameter>
        <Parameter>
            <Name>RMS in GSM field components</Name>
            <ParameterKey>RMS in GSM field components</ParameterKey>
            <Description>RMS deviation in 1-min averages of GSM field components</Description>
            <Cadence>PT1M</Cadence>
            <Units>nT</Units>
            <CoordinateSystem>
                <CoordinateRepresentation>Cartesian</CoordinateRepresentation>
                <CoordinateSystemName>GSM</CoordinateSystemName>
            </CoordinateSystem>
            <Measured>
                <Field>
                    <FieldQualifier>Variance</FieldQualifier>
                    <FieldQuantity>Magnetic</FieldQuantity>
                </Field>
            </Measured>
        </Parameter>
        <Parameter>
            <Name>RMS in field magnitude</Name>
            <ParameterKey>RMS in field magnitude</ParameterKey>
            <Description>RMS deviation in field magnitude</Description>
            <Cadence>PT1M</Cadence>
            <Units>nT</Units>
            <Measured>
                <Field>
                    <FieldQualifier>Variance</FieldQualifier>
                    <FieldQuantity>Magnetic</FieldQuantity>
                </Field>
            </Measured>
        </Parameter>
        <Parameter>
            <Name>Time</Name>
            <ParameterKey>Time</ParameterKey>
            <Description>Time at midpoint of average, given as year, month, day, hour, minute, second</Description>
            <Cadence>PT1M</Cadence>
            <Support>Temporal</Support>
        </Parameter>
        <Parameter>
            <Name>Number of points in average</Name>
            <ParameterKey>Number of points in average</ParameterKey>
            <Description>Number of fine scale points in average</Description>
            <Support>Other</Support>
        </Parameter>
        <Parameter>
            <Name>GSE Wind position</Name>
            <ParameterKey>GSE Wind position</ParameterKey>
            <Description>Cartesian components of Wind position vector</Description>
            <Cadence>PT1M</Cadence>
            <Units>Re</Units>
            <CoordinateSystem>
                <CoordinateRepresentation>Cartesian</CoordinateRepresentation>
                <CoordinateSystemName>GSE</CoordinateSystemName>
            </CoordinateSystem>
            <Support>Positional</Support>
        </Parameter>
        <Parameter>
            <Name>GSM Wind position</Name>
            <ParameterKey>GSM Wind position</ParameterKey>
            <Description>Y and Z components of Wind position vector.  Xgsm = Xgse</Description>
            <Cadence>PT1M</Cadence>
            <Units>Re</Units>
            <CoordinateSystem>
                <CoordinateRepresentation>Cartesian</CoordinateRepresentation>
                <CoordinateSystemName>GSM</CoordinateSystemName>
            </CoordinateSystem>
            <Support>Positional</Support>
        </Parameter>
    </NumericalData>
</Spase>
